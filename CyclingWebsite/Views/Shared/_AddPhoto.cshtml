@model PhotoDto

<form asp-controller="Tour" asp-action="AddPhoto" method="post" enctype="multipart/form-data" class="form-group">

    <div class="form-group">
        <input asp-for="@Model.Description" />
    </div>
    <div class="form-group">
        <input asp-for="@Model.File" />
    </div>
    <input type="hidden" asp-for="@Model.TourId" />
    <button type="submit" class="btn btn-primary">Add</button>
</form>


